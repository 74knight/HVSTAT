		"resources/settings-pane.html",
		"images/channeling.png",
		"images/healthpot.png",
		"images/manapot.png",
		"images/spiritpot.png",
		"images/ui-bg_flat_0_aaaaaa_40x100.png",
		"images/ui-bg_flat_55_fbf9ee_40x100.png",
		"images/ui-bg_flat_65_edebdf_40x100.png",
		"images/ui-bg_flat_75_e3e0d1_40x100.png",
		"images/ui-bg_flat_75_edebdf_40x100.png",
		"images/ui-bg_flat_95_fef1ec_40x100.png",
		"images/ui-icons_2e83ff_256x240.png",
		"images/ui-icons_5c0d11_256x240.png",
		"images/ui-icons_cd0a0a_256x240.png"
	]
}
